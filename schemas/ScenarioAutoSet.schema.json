{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ScenarioAutoSet",
  "type": "object",
  "properties": {
    "scenarios": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "title": { "type": "string" },
          "summary": { "type": "string" },
          "state_vector": { "type": "object" }
        },
        "required": ["title", "summary", "state_vector"]
      }
    }
  },
  "required": ["scenarios"]
}
