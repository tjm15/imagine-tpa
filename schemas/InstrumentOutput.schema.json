{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "InstrumentOutput",
    "type": "object",
    "properties": {
        "run_id": {
            "type": "string",
            "format": "uuid"
        },
        "instrument_id": {
            "type": "string"
        },
        "inputs_logged": {
            "type": "object"
        },
        "output_data": {
            "type": "object"
        },
        "limitations_statement": {
            "type": "string"
        },
        "timestamp": {
            "type": "string",
            "format": "date-time"
        }
    },
    "required": [
        "run_id",
        "instrument_id",
        "limitations_statement"
    ]
}
