{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "PlanRealityInterpretation",
  "type": "object",
  "description": "An interpretation of planâ†”reality registration outputs, including uncertainty and what the overlay indicates in this context.",
  "properties": {
    "plan_reality_interpretation_id": { "type": "string", "format": "uuid" },
    "visual_asset_id": { "type": "string", "format": "uuid" },
