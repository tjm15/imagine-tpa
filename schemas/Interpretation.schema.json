{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Interpretation",
    "type": "object",
    "properties": {
        "interpretation_id": {
            "type": "string",
            "format": "uuid"
        },
        "claim": {
            "type": "string"
        },
        "evidence_refs": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "assumptions_used": {
            "type": "array",
            "items": {
                "type": "string",
                "format": "uuid"
            }
        },
        "limitations_text": {
            "type": "string"
        }
    },
    "required": [
        "interpretation_id",
        "claim",
        "evidence_refs"
    ]
}